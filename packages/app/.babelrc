{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "react-hot-loader/babel",
    ["relay", { "artifactDirectory": "./src/__generated__" }]
  ],
  "env": {
    "production": {
      "presets": [
        "minify"
      ]
    }
  }
}
